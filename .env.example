# Configuration pour CompareDB
# Copiez ce fichier vers .env et modifiez les valeurs selon votre configuration

# =========================
#  API Configuration (Mode en ligne)
# =========================

# Snowflake Embeddings API
SNOWFLAKE_API_BASE=https://api.dev.dassault-aviation.pro/snowflake-arctic-embed-l-v2.0/v1
SNOWFLAKE_API_KEY=token

# DALLEM LLM API
DALLEM_API_BASE=https://api.dev.dassault-aviation.pro/dallem-pilote/v1
DALLEM_API_KEY=EMPTY

# Reranker API (optionnel)
RERANK_API_BASE=https://api.dev.dassault-aviation.pro/bge-reranker-v2-m3/v1/
RERANK_API_KEY=EMPTY

# SSL Verification
DISABLE_SSL_VERIFY=true

# =========================
#  Application Configuration
# =========================

# Flask
FLASK_HOST=0.0.0.0
FLASK_PORT=5000
FLASK_DEBUG=1

# Logging
LOG_LEVEL=INFO
DEBUG=0
